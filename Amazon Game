local world = workspace.World print('ran') for i,v in world:GetChildren() do if v.Name:match('Gift') then game.Players.LocalPlayer.Character:PivotTo(v.Collider.CFrame) task.wait() end end for i,v in world.Area01MagicalForest.GiftsZone01:GetChildren() do if v.Name:match('Gift') then game.Players.LocalPlayer.Character:PivotTo(v.Collider.CFrame) task.wait() end end for i,v in world.Area02StarrySkies.GiftsZone02:GetChildren() do if v.Name:match('Gift') then game.Players.LocalPlayer.Character:PivotTo(v.Collider.CFrame) task.wait() end end for i,v in world.Area02StarrySkies.World.TelescopeParts:GetDescendants() do if v:IsA('BasePart') then game.Players.LocalPlayer.Character:PivotTo(v.CFrame) task.wait(0.4) end end for i,v in world.Area02StarrySkies.World.HoneyPots:GetDescendants() do if v:IsA('BasePart') then game.Players.LocalPlayer.Character:PivotTo(v.CFrame) task.wait(0.4) end end
